    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-junit": "^15.0.0",
    "jest-prosemirror": "^1.0.10",
    "lint-staged": "^13.0.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "write-file-webpack-plugin": "^4.5.1",
    "terser-webpack-plugin": "^5.3.6"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist/ && rm -f modusoperandi-licit-export-pdf-*.*.*.tgz",
    "webpack": "webpack",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts",
    "build:dist": "npm run clean && npm run build:ts && npm run webpack && npm run build:babel",
    "lint:ts": "eslint src --ext .ts --fix",
    "lint:css": "echo \"No CSS available\" && exit 1",
    "lint": "npm run lint:css & npm run lint:ts",
    "ci:build": "npm run clean && npm run build:ts && npm run webpack && npm run build:babel",
    "ci:bom": "cyclonedx-npm --ignore-npm-errors --short-PURLs --output-format XML --output-file dist/bom.xml",
    "prepare": "npm run build:dist",
    "publish:dist": "npm publish"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*test|*.setup).ts": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
