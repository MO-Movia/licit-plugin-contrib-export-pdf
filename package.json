{
  "name": "@modusoperandi/licit-export-pdf",
  "version": "1.4.5",
  "type": "module",
  "description": "MO's Export To PDF ProseMirror Plugin",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MO-Movia/licit-plugin-contrib-export-pdf"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist/ && rm -f modusoperandi-licit-export-pdf-*.*.*.tgz",
    "lint:ts": "eslint src --ext .ts --fix",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint": "npm run lint:css & npm run lint:ts",
    "lint:fix": "npm run lint:css --fix & npm run lint:ts --fix",
    "build": "tsc --build && copyfiles --up 1 \"src/**/*.css\" dist  && copyfiles package.json dist",
    "ci:build": "tsc --build && copyfiles --up 1 \"src/**/*.css\" dist && copyfiles --up 1 \"src/assets/**\" dist && copyfiles package.json LICENSE README.md dist",
    "ci:bom": "npx cyclonedx-npm --ignore-npm-errors --short-PURLs --output-format XML --output-file dist/bom.xml",
    "verify": "npm run lint -- --fix && npm run ci:build && npm run test:coverage && echo 'All Tests Passed!'"
  },
  "peerDependencies": {
    "@modusoperandi/licit-doc-attrs-step": "^1.0.3",
    "@modusoperandi/licit-ui-commands": "^1.0.8",
    "pagedjs": "^0.4.3",
    "prosemirror-model": "^1.22.2",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "html2canvas": "^1.4.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-prosemirror": "^3.0.1",
    "lint-staged": "^16.2.6",
    "moment": "^2.30.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "!(*test|*.setup).ts": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
